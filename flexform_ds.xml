					<currencyCode>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.currencyCode</label>
							<config>
								<type>select</type>
								<items>
								</items>
								<itemsProcFunc>tx_staticinfotables_div->selectItemsTCA</itemsProcFunc>
								<itemsProcFunc_config>
									<table>static_currencies</table>
									<where> AND cu_iso_3 IN (USD, EUR)</where>
									<indexField>cu_iso_3</indexField>
									<prependHotlist>1</prependHotlist>
								</itemsProcFunc_config>
								<size>1</size>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
							</config>
						</TCEforms>
					</currencyCode>
					-->
					<amount>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>string</eval>
							</config>
						</TCEforms>
					</amount>
					<amounts>
						<type>array</type>
						<section>1</section>
						<el>
							<data>
								<TCEforms>
									<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount</label>
								</TCEforms>
								<type>array</type>
								<el>
									<value>
										<TCEforms>
											<exclude>1</exclude>
											<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount_value</label>
											<config>
												<type>input</type>
												<size>6</size>
												<max>6</max>
												<eval>integer</eval>
											</config>
										</TCEforms>
									</value>
									<label>
										<TCEforms>
											<exclude>1</exclude>
											<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount_label</label>
											<config>
												<type>input</type>
												<size>10</size>
												<max>20</max>
												<eval>string</eval>
											</config>
										</TCEforms>
									</label>
								</el>
							</data>
						</el>
					</amounts>
					<returnUrl>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.returnUrl</label>
							<config>
								<type>input</type>
								<size>50</size>
								<eval>string</eval>
							</config>
						</TCEforms>
					</returnUrl>
					<itemName>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.itemName</label>
							<config>
								<type>input</type>
								<size>50</size>
								<eval>string</eval>
							</config>
						</TCEforms>
					</itemName>
					<donateText>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.donateText</label>
							<config>
								<type>text</type>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</donateText>
				</el>
			</ROOT>
		</general>
	</sheets>
</T3DataStructure>
