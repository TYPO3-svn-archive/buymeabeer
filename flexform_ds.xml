					<amount>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</amount>
					<amounts>
						<tx_templavoila>
							<title>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.more_amounts</title>
						</tx_templavoila>
						<type>array</type>
						<section>1</section>
						<el>
							<data>
								<type>array</type>
								<el>
									<value>
										<TCEforms>
											<exclude>1</exclude>
											<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount_value</label>
											<config>
												<type>input</type>
												<size>6</size>
												<max>6</max>
												<eval>integer</eval>
											</config>
										</TCEforms>
									</value>
									<label>
										<TCEforms>
											<exclude>1</exclude>
											<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.amount_label</label>
											<config>
												<type>input</type>
												<size>10</size>
												<max>20</max>
												<eval>string</eval>
											</config>
										</TCEforms>
									</label>
								</el>
							</data>
						</el>
					</amounts>
					<returnUrl>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.returnUrl</label>
							<config>
								<type>input</type>
								<size>50</size>
								<max>255</max>
								<eval>trim</eval>
								<wizards>
									<_PADDING>2</_PADDING>
									<link>
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard</script>
										<JSopenParams>height=500,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</returnUrl>
					<itemName>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.itemName</label>
							<config>
								<type>input</type>
								<size>50</size>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</itemName>
					<donateText>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.donateText</label>
							<config>
								<type>text</type>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</donateText>
				</el>
