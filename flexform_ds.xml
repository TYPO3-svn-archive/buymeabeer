					<transitionDuration>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.transitionDuration</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<default></default>
								<checkbox>0</checkbox>
								<eval>int</eval>
							</config>
						</TCEforms>
					</transitionDuration>
