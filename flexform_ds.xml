<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<overrideSetup>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.overrideSetup</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</overrideSetup>
					<business>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.business</label>
							<config>
								<type>input</type>
								<size>50</size>
								<eval>email</eval>
							</config>
						</TCEforms>
					</business>
					<currencyCode>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:buymeabeer/locallang_db.xml:tt_content.pi_flexform.currencyCode</label>
							<config>
								<type>input</type>
								<size>6</size>
